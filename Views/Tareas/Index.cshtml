@model IEnumerable<Organizacional.Models.Tarea>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaAsignacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaEjecucion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdDocumentoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTecnicoAsignadoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaAsignacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaEjecucion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdDocumentoNavigation.IdDocumento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTecnicoAsignadoNavigation.IdUsuario)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdTarea">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdTarea">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdTarea">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
